@inproceedings { 681,
	title = {Peripheral-Foveal Vision for Real-time Object Recognition and Tracking in Video},
	year = {2007},
	abstract = {<p>
<p>Human object recognition in a physical 3-d envronment is still far superior to that of any robotic vision system.~We believe that one reason (out of many) for this{\textemdash}one that has not heretofore been significantly exploited in the~artificial vision literature{\textemdash}is that humans use a fovea to fixate on, or near an object, thus obtaining a very high~resolution image of the object and rendering it easy to recognize. In this paper, we present a novel method for~identifying and tracking objects in multi-resolution digital video of partially cluttered environments. Our method~is motivated by biological vision systems and uses a learned {\textquotedblleft}attentive{\textquotedblright} interest map on a low resolution data stream~to direct a high resolution {\textquotedblleft}fovea.{\textquotedblright} Objects that are recognized in the fovea can then be tracked using peripheral~vision. Because object recognition is run only on a small foveal image, our system achieves performance in real-time~object recognition and tracking that is well beyond simpler systems.</p>
</p>},
	author = {Andrew Y. Ng and Gary Bradski and Stephen Gould and Marius Messner and Paul Baumstarck and Sukwon Chung and Joakim Arfvidsson and Adrian Kaehler and Benjamin Sapp}
}

