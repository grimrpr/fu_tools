@article { 678,
	title = {Stanley: The Robot that Won the DARPA Grand Challenge},
	journal = {Journal of Field Robotics},
	year = {2006},
	month = {2006},
	abstract = {<p>
<p>This article describes the robot Stanley, which won the 2005 DARPA Grand Challenge.~Stanley was developed for high-speed desert driving without manual intervention. The~robot{\textquoteright}s software system relied predominately on state-of-the-art artificial intelligence~technologies, such as machine learning and probabilistic reasoning. This paper describes~the major components of this architecture, and discusses the results of the Grand Challenge race.</p>
</p>},
