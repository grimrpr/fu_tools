@article { 362,
	title = {Robot-Vision Signal Processing Primitives},
	volume = {130},
	year = {2008},
	month = {1/2008},
	type = {Applications Corner},
	abstract = {<p>Recent advances in vision algorithms and increases in computer
performance have made new capabilities available in autonomous robotics
for real-time applications. In general,~~computer-vision-based
